# Docker环境变量配置示例
# 复制此文件为 .env 并填入真实值

# SiliconFlow AI API配置
SILICONFLOW_API_KEY=your_siliconflow_api_key_here
SILICONFLOW_API_URL=https://api.siliconflow.cn/v1/chat/completions

# 应用配置
NODE_ENV=production
PORT=3000

# 数据库配置
DB_PATH=/app/data/logistics.db

# 日志配置
LOG_LEVEL=info
LOG_DIR=/app/logs

# Docker特定配置
COMPOSE_PROJECT_NAME=wlbj
